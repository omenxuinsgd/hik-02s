{
  "version": 2,
  "builds": [
    {
      "src": "apps/api-harmonization/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "apps/frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/apps/api-harmonization/dist/main.js"
    },
    {
      "src": "/(.*)",
      "dest": "/apps/frontend"
    }
  ]
}

